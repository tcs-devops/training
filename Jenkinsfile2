pipeline {
    agent any
    stages {
        stage("Build") {
            steps {
                echo "Building"
                echo "Building finished"
                
                    
            }
        }
        
        stage("Test") {
            steps {
            checkout ([$class: 'GitSCM',
            branches: [[name: '*/jenkins-session' ]],
            userRemoteConfigs: [[url: 'https://github.com/jenkins-docs/simple-java-maven-app.git]]])
                
            }
        }
        
        stage("Deployment") {
            steps{
                echo "Deploying"
                echo "Deployment finished"
                
            }
        }
          
    }
}
